{
  "name": "live627/elkarte-addon-helper",
  "description": "Collection of scripts to act as the backbone for ElkArte addons",
  "keywords": [
    "ElkArte",
    "addons"
  ],
  "homepage": "https://github.com/live627/elkarte-addon-helper",
  "license": "MIT",
  "authors": [
    {
      "name": "John Rayes",
      "email": "live627@gmail.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/elkarte/Elkarte.git",
      "type": "vcs"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "suki/ohara": "1.0.*",
    "paragonie/random_compat": "~2.0",
    "mnapoli/simplex": "0.2.1",
    "ezyang/htmlpurifier": "~4.8",
    "symfony/http-foundation": "~3.1",
    "elkarte/elkarte": "dev-development"
  },
  "require-dev": {
    "phpunit/phpunit": "~4|~5"
  },
  "autoload": {
    "psr-4": {
      "live627\\AddonHelper\\": "src/"
    }
  },
  "minimum-stability": "dev"
}
